<?xml version="1.0" encoding="utf-8" ?>
<rxui:ReactiveContentPage  
    x:Class="WhatsBack.MainPage"  
    x:TypeArguments="whatsBack:MainPageViewModel"
    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"  
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"  
    xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"   
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:whatsBack="clr-namespace:WhatsBack;assembly=WhatsBack"
    ios:Page.UseSafeArea="true">
    <StackLayout>
        <Button x:Name="LoadFile" Text="Load file" TextColor="White" BackgroundColor="Gray" />
        <Button x:Name="ScanFolder" Text="Scan folder" TextColor="White" BackgroundColor="Gray" />
    </StackLayout>
</rxui:ReactiveContentPage>  
